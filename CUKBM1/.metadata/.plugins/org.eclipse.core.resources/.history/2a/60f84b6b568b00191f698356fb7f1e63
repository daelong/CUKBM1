				throw new Exception("참가인원을 설정하세요.");
		      }
		} catch(Exception e) {
			System.out.println(e.toString());
		}
		finally {
			try {
				stmt.close();
			}
			catch (Exception ignored) {}
			try {
				conn.close();
			}
			catch (Exception ignored) {}
		}
		
		response.sendRedirect("/cukbm/Board?SUBJECT="+subject+"&TEAM="+team);
	}
}
